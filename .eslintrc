{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "react/jsx-props-no-spreading": "off",
    "react/prop-types": "off",
    "no-plusplus": "off",
    "jsx-a11y/click-events-have-key-events": "off"
  }
}